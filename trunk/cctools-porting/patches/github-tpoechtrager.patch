-typedef union {
-	double 		__dbl;
-	ppc_fp_scr_t 	__scr;
-} __fp_un_t;
-
-static __inline__ ppc_fp_scr_t
-get_fp_scr()
-{
-	__fp_un_t 	__fp_un;
-	
-	__asm__ volatile ("mffs. %0           /* mffs */"	\
-	  	: "=f" (__fp_un.__dbl));
-	return (__fp_un.__scr);		
-}
-
-static __inline__ void
-set_fp_scr(ppc_fp_scr_t fp_scr)

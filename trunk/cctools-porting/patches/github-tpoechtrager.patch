+		   mach/m88k/thread_status.h \
+		   architecture/m88k/fp_regs.h \
+		   architecture/m88k/reg_help.h \
+		   architecture/nrw/reg_help.h \
+		   architecture/nrw/macro_help.h \
+		   mach/hppa/thread_status.h \
+		   mach/sparc/thread_status.h \

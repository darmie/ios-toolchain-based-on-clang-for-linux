-/*
- * Macro package initialization
- */
-	.set	__no_at,0		/* allow at by default */
-
-/*
- * .at_off -- disable use of at by macros
- * .at_on -- enable use of at by macros
- */
-.macro	.at_off
-	.set	__no_at,1
-.endmacro
-
-.macro	.at_on

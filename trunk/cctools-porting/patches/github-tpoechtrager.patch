-/*******************************************************************************
-*                                                                              *
-*     File:  fenv.h	                                                       *
-*                                                                              *
-*     Contains: typedefs and prototypes for C99 floating point environment.    *
-*                                                                              *
-*******************************************************************************/
-
-#ifndef __FENV__
-#define __FENV__
-
-#if defined( __i386__ ) || defined( __x86_64__ )
-    #error  Wrong arch. This is PowerPC only.
-#endif
-
-#if defined(__GNUC__) && (__GNUC__ >= 4)   
-#pragma GCC fenv
-#endif
